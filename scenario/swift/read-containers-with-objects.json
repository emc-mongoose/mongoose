						"item" : {
							"dst" : {
								"container" : "9"
							}
						}
					}
				}
			]
		}, {
			// read 10 directories with the files
			"type" : "load",
			"config" : {
				"item" : {
					"src" : {
						"file" : "/tmp/scenario-swift-read-containers-with-objects.json"
					}
				},
				"load" : {
					"type" : "read"
				}
			}
