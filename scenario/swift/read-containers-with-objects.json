{
	"type" : "sequential",
	"config" : {
		"item" : {
			"type" : "container"
		},
		"load" : {
			"limit" : {
				"count" : 10
			},
			"threads" : 10
		},
		"storage" : {
			"http" : {
				"api" : "Swift",
				"namespace" : "test"
			},
			"port" : 9024
		}
	},
	"jobs" : [
		{
			// create 10 directories
			"type" : "precondition",
			"config" : {
				"item" : {
					"dst" : {
