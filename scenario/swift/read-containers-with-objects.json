							}
						}
					}
				}, {
					"type" : "precondition",
					"config" : {
						"item" : {
							"dst" : {
								"container" : "4"
							}
						}
					}
				}, {
					"type" : "precondition",
					"config" : {
						"item" : {
							"dst" : {
								"container" : "5"
							}
						}
					}
				}, {
					"type" : "precondition",
