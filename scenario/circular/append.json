{
	"type" : "sequential",
	"config" : {
		"item" : {
			"data" : {
				"size" : "16KB"
			}
		},
		"storage" : {
			"driver" : {
				"concurrency" : 10
			}
		}
	},
	"steps" : [
		{
			"type" : "precondition",
			"config" : {
				"item" : {
					"output" : {
						"file" : "circular-append-before.csv"
					}
				},
				"test" : {
					"step" : {
						"limit" : {
							"count" : 1000
						}
					}
				}
			}
		},
		{
			"type" : "load",
			"config" : {
				"item" : {
					"data" : {
						"ranges" : {
							"fixed" : [
								"-16KB-"
							]
						}
					},
					"input" : {
						"file" : "circular-append-before.csv"
					},
					"output" : {
						"file" : "circular-append-after.csv"
					}
				},
				"load" : {
					"type" : "update",
					"circular" : true
				},
				"test" : {
					"step" : {
						"limit" : {
							"count" : 31000
						}
					}
				}
			}
		}
	]
}
